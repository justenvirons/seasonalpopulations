  file = {C\:\\Users\\scott\\OneDrive - DePaul University\\LITERATURE\\Zotero\\storage\\IC93UJMG\\Checker - 2011 - Wiped Out by the â€œGreenwaveâ€ Environmental Gentri.pdf;C\:\\Users\\scott\\OneDrive - DePaul University\\LITERATURE\\Zotero\\storage\\5VVPVRU7\\j.1548-744X.2011.01063.html},
