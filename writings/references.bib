@article{palmAchievingRegionalHousing2017,
  title = {Achieving {{Regional Housing Planning Objectives}}: {{Directing Affordable Housing}} to {{Jobs}}-{{Rich Neighborhoods}} in the {{San Francisco Bay Area}}},
