@article{sasakiValidationComparisonActiGraph2011,
  title = {Validation and Comparison of {{ActiGraph}} Activity Monitors},
  author = {Sasaki, Jeffer E. and John, Dinesh and Freedson, Patty S.},
  year = {2011},
  month = sep,
  volume = {14},
