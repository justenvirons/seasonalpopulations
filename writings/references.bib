@article{knutsCyclingCityBelgian2012,
  title = {Cycling in the {{City}}? {{Belgian Cyclists Conquering Urban Spaces}}, 1860\textendash 1900},
  shorttitle = {Cycling in the {{City}}?},
  author = {Knuts, Stijn and Delheye, Pascal},
